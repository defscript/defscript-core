{
  "name": "defscript-core",
  "type": "module",
  "version": "0.1.0",
  "description": "A simple, solid compile-to-js language with a extension API",
  "main": "./lib/index.mjs",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run build-parser && npm run test-light",
    "test-light": "node --experimental-modules --es-module-specifier-resolution=node test/index.mjs",
    "repl": "node --experimental-modules --es-module-specifier-resolution=node test/repl.js",
    "install": "npm run build-parser",
    "build-parser": "crane grammar.crane -r module > lib/parser.out.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielratener/defscript.git"
  },
  "keywords": [
    "web",
    "javascript",
    "compile-to-js"
  ],
  "author": "Gabriel Ratener",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defscript/defscript-core/issues"
  },
  "homepage": "https://github.com/defscript/defscript-core#readme",
  "dependencies": {
    "crane-parser": "^0.2.0",
    "esm": "^3.2.25",
    "estemplate": "^0.5.1",
    "lexie": "^1.0.6"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "livescript": "^1.5.0"
  }
}
